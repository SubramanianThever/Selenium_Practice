<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name= "Groupshandling">
	<test name="GroupsTest">
		<groups>
			<run>
				<include name="Sanity"/>
				<include name="Regression"/>
				<exclude name="Regression"/>
				<include name="functional"/>
			</run>
		</groups>
		<classes>
			<class name="TestNGPractice.LoginGroupingHandling"/>
			<class name="TestNGPractice.SignupGroupingHandling"/>
			<class name="TestNGPractice.PaymentGroupingHandling"/>
		</classes>
	</test>
</suite>